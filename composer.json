{
    "name": "olvlvl/phpunit-given",
    "type": "library",
    "description": "An alternative to PHPUnit's ReturnValueMap and ReturnCallback. A convenient solution to migrate from Prophecy.",
    "keywords": [
        "phpunit",
        "mock",
        "test double"
    ],
    "homepage": "https://github.com/olvlvl/phpunit-given",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Olivier Laviale",
            "email": "olivier.laviale@gmail.com",
            "homepage": "https://olvlvl.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/olvlvl/phpunit-given/issues",
        "source": "https://github.com/olvlvl/phpunit-given"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "phpunit/phpunit": "^11.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "olvlvl\\Given\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\olvlvl\\Given\\": "tests"
        }
    }
}
